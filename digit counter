import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int count=0;
        if(a==0){
            count = 1;
        }
        for(int i=a; i!=0; i=i/10){
            
            count++;
        }
        System.out.println(count);
        sc.close();
    }
}
